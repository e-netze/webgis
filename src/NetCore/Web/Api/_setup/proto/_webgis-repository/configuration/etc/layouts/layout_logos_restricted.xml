		</panel>
		</panel>
		
		
		<!-- <panel dock="fill"  border="none" bordercolor="0,0,0"> -->
			<legend dock="fill" />
		<!-- </panel> -->
		
		
		
	</panel>
	
	



<map dock="fill" border="all" bordercolor="0,0,0">
</map>

<panel dock="top" border="none" bordercolor="0,0,0" height="25">

	<panel dock="right" border="none" bordercolor="0,0,0" width="40">

		<panel dock="fill" border="all" bordercolor="0,0,0" fillcolor="255,255,255" >

			<!-- <text string="übersichtskarte" align="center" font="arial" y="6" fontcolor="0,0,0" fontsize="2.5"/> -->
			<ovmap  border="all" dock='fill' bordercolor="0,0,0" />
		</panel>

	</panel>


</panel>


<panel dock="bottom" border="none" bordercolor="0,0,0" height="6.5">
	<scalebar dock="left" width="65" border="all" bordercolor="0,0,0" fillcolor="240,240,240"  />
	<northarrow dock="right" width="7" height="8.5"  border="all" bordercolor="0,0,0" fillcolor="240,240,240"/>
</panel>


</panel>


</panel>




  <variables>
	<variable name="TITLE" alias="Karten-Titel" />
	<variable name="SUBT" alias="Karten-Untertitel" />
	<variable name="ERSTELLER" alias="Ersteller"/> 
	<variable name="ADR" alias="Adresse"/>
	<variable name="PROJEKT" alias="Projekt.Nr."/> 
	<variable name="DIV" alias="Diverses"/> 


  </variables>





</layout>
