		</panel>
	

	</panel>	
	
	
	
	
	
	<panel dock="fill" border="all" bordercolor="0,0,0" fillcolor="255,255,255">
	
	<panel dock="right" width="50" border="none" bordercolor="0,0,0">
	
		<panel dock="left" width="1" border="none"  bordercolor="0,0,0"/>
	

		
		<panel dock="top" height="10" border="none" bordercolor="0,0,0">
			<text string="Legende: " font="Arial" y="2" fontcolor="0,0,0" fontsize="2.5"/>
		</panel>
		
			
			
		<panel dock="bottom" height="28" border="top" bordercolor="0,0,0" >
			<panel dock="fill"  border="none" bordercolor="0,0,0">

			<text string="Adresse: [ADR]" y="1" x="0" font="Arial"  fontcolor="0,0,0" fontsize="2.0"/>
			<text string="Projekt Nr.: [PROJEKT]" y="5" x="0" font="Arial"  fontcolor="0,0,0" fontsize="2.0"/>
			<text string="Diverses: [DIV]" y="9" x="0" font="Arial"  fontcolor="0,0,0" fontsize="2.0"/>	
			<text string="Erstellt von: [ERSTELLER]" y="13" x="0" font="Arial"  fontcolor="0,0,0" fontsize="2.0"/>
			<text string="Datum: [DATE]" y="17" x="0" font="Arial"  fontcolor="0,0,0" fontsize="2.0"/>
			<text string="Anzeigemaßstab: [SCALE] " font="Arial" y="21" fontcolor="0,0,0"  fontsize="2.0"/>
