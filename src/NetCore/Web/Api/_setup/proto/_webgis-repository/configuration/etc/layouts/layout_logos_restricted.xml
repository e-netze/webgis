<?xml version="1.0" encoding="iso-8859-1"?>
<layout scales="500,1000"
        page_sizes="A4"
		page_orientations="Landscape,Portrait"	>
<panel dock="fill" border="none" bordercolor="0,0,0" fillcolor="255,255,255">
	
	<panel dock="top" height="20" border="none" bordercolor="0,0,0">
		
		<panel dock="left" width="15" border="none" bordercolor="0,0,0">
			<image src="img/logos/e.png" align="center"  width="13.14" height="10" />
		</panel>
		
		<panel dock="right" width="40" border="none" bordercolor="0,0,0">
			<image src="img/logos/basemap_at.png" align="center"  width="36" height="10" />
		</panel>
		
		
		
		<panel dock="fill"  border="none" bordercolor="0,0,0">
			<text string="[TITLE]" align="center" y="2"  font="Calibri" fontstyle="bold" fontsize="5" fontcolor="0,0,0" />
			<text string="[SUBT]" align="center" y="10"  font="Calibri"  fontsize="4" fontcolor="0,0,0" />
		</panel>
		
	</panel>
	
	
	
	
	<panel dock="bottom" height="8" border="none" bordercolor="0,0,0" fillcolor="255,255,255">
